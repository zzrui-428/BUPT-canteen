/**app.wxss**/
.container{
 background-color: #fcf7f3;
 
 display: flex;
 text-align: center;
 flex-direction: column;
 justify-content: space-around;
 align-items: center;
}
page {
  background: #fcf7f3;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.sectionTitle{
    font-size: 70rpx;
    color:rgb(218, 90, 51);
    text-align: center;
    font-weight:bold;
    font-family:Impact, Haettenschweiler, ‘ArialNarrowBold’, sans-serif;
    margin-bottom:40rpx;
    margin-top:40rpx; 
}

.shadow{
  background-color: #ffffff;
  display: inline-block;
  margin:30rpx;
  border-radius:20rpx;
  padding: 10px;
  box-shadow: 0rpx 4rpx 15rpx  #ccc;
}